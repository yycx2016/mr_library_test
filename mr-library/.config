# CONFIG_MR_USING_PWM10 is not set
# CONFIG_MR_USING_PWM11 is not set
# CONFIG_MR_USING_PWM12 is not set
# CONFIG_MR_USING_PWM13 is not set
# CONFIG_MR_USING_PWM14 is not set
# end of PWM

#
# UART
#
CONFIG_MR_USING_UART1=y
# CONFIG_MR_USING_UART2 is not set
# CONFIG_MR_USING_UART3 is not set
# CONFIG_MR_USING_UART4 is not set
# CONFIG_MR_USING_UART5 is not set
# end of UART

#
# SPI
#
# CONFIG_MR_USING_SPI1 is not set
# CONFIG_MR_USING_SPI2 is not set
# CONFIG_MR_USING_SPI3 is not set
# end of SPI

#
# Timer
#
# CONFIG_MR_USING_TIMER1 is not set
# CONFIG_MR_USING_TIMER2 is not set
# CONFIG_MR_USING_TIMER3 is not set
# CONFIG_MR_USING_TIMER4 is not set
# CONFIG_MR_USING_TIMER5 is not set
# CONFIG_MR_USING_TIMER6 is not set
# CONFIG_MR_USING_TIMER7 is not set
# CONFIG_MR_USING_TIMER8 is not set
# CONFIG_MR_USING_TIMER9 is not set
