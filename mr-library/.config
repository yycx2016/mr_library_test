
#
# System configure
#
CONFIG_MR_USING_ASSERT=y
CONFIG_MR_CFG_HEAP_SIZE=4096

#
# Log configure
#
CONFIG_MR_USING_LOG_ERROR=y
CONFIG_MR_USING_LOG_WARN=y
CONFIG_MR_USING_LOG_INFO=y
CONFIG_MR_USING_LOG_DEBUG=y
CONFIG_MR_USING_LOG_SUCCESS=y
# CONFIG_MR_USING_LOG_COLOR is not set
# end of Log configure

#
# Printf configure
#
CONFIG_MR_CFG_PRINTF_BUFSZ=128
CONFIG_MR_CFG_PRINTF_DEV_NAME="serial1"
# CONFIG_MR_USING_PRINTF_NONBLOCKING is not set
# end of Printf configure
# end of System configure

#
# Device configure
#
CONFIG_MR_CFG_DEV_NAME_LEN=12
CONFIG_MR_CFG_DESC_NUM=64
CONFIG_MR_USING_DESC_CHECK=y
CONFIG_MR_USING_RDWR_CTL=y

#
# Device configure
#
# CONFIG_MR_USING_ADC is not set
# CONFIG_MR_USING_CAN is not set
# CONFIG_MR_USING_DAC is not set
# CONFIG_MR_USING_I2C is not set
# CONFIG_MR_USING_PIN is not set
# CONFIG_MR_USING_PWM is not set
CONFIG_MR_USING_SERIAL=y

#
# Serial configure
#
CONFIG_MR_CFG_SERIAL_RD_BUFSZ=32
CONFIG_MR_CFG_SERIAL_WR_BUFSZ=0
# CONFIG_MR_USING_SERIAL_DMA is not set
# end of Serial configure

# CONFIG_MR_USING_SPI is not set
# CONFIG_MR_USING_TIMER is not set
# end of Device configure

#
# Driver configure
#

#
# ADC
#
# CONFIG_MR_USING_ADC1 is not set
# CONFIG_MR_USING_ADC2 is not set
# CONFIG_MR_USING_ADC3 is not set
# end of ADC

#
# DAC
#
# CONFIG_MR_USING_DAC1 is not set
# end of DAC

#
# PWM
#
# CONFIG_MR_USING_PWM1 is not set
# CONFIG_MR_USING_PWM2 is not set
# CONFIG_MR_USING_PWM3 is not set
# CONFIG_MR_USING_PWM4 is not set
# CONFIG_MR_USING_PWM5 is not set
