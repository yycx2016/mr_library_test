# end of Driver configure

#
# Components configure
#
CONFIG_MR_USING_MSH=y

#
# Msh configure
#
CONFIG_MR_CFG_MSH_BUFSZ=32
CONFIG_MR_CFG_MSH_ARGS_NUM=16
CONFIG_MR_CFG_MSH_HISTORY_LINES=4
CONFIG_MR_CFG_MSH_DEV_NAME="serial1"
# CONFIG_MR_CFG_MSH_NONBLOCKING is not set
CONFIG_MR_CFG_MSH_PRINTF_BUFSZ=128
CONFIG_MR_CFG_MSH_PROMPT="msh"
# CONFIG_MR_USING_MSH_PRINTF_COLOR is not set
CONFIG_MR_USING_MSH_ECHO=y
CONFIG_MR_USING_MSH_DEV_CMD=y
# end of Msh configure
# end of Components configure
